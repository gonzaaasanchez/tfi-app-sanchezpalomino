{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "config": "preview.yml"
      },
      "ios": {
        "image": "latest",
        "config": "preview.yml"
      }
    },
    "regression": {
      "extends": "preview",
      "android": {
        "config": "regression.yml"
      },
      "ios": {
        "simulator": true,
        "config": "regression.yml"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
